  "html": "<style>\n\t.print-format table, .print-format tr,\n\t.print-format td, .print-format div, .print-format p {\n\t\tfont-family: Monospace !important;\n\t\tline-height: 100% !important;\n\t\tvertical-align: middle !important;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 4in !important;\n\t\t\tpadding: 0.10in !important;\n\t\t\tmin-height: 11.8in !important;\n\t\t}\n\t}\n</style>\n\n<p class=\"text-center\">\n\t<b>{{ company }}</b><br>\n\t{% if company_address_value %}\n\t\t<br>\n\t\t{{ company_address_value }}\n\t{% endif %}\n\t\n\t{% if phone_number %}\n\t\t<br>\n\t\tTEL:{{ phone_number }}\n\t{% endif %}\n\t{% if vat %}\n\t\t<br>\n\t\t<b>{{ vat }}</b>\n\t{% endif %}\n\n\n<p>\n\n<table width=\"100%\" style=\"border-top: 1px solid black\">\n\t<thead>\n\t\t<tr>\n\t\t\t<td width=\"20%\">{{ __(\"SI\") }}</td>\n\t\t\t<td width=\"50%\">{{ __(\"Name\") }}</td>\n\t\t\t<td width=\"15%\" class=\"text-right\">{{ __(\"Qty\") }}</td>\n\t\t\t<td width=\"15%\" class=\"text-right\">{{ __(\"Amount\") }}</td>\n\t\t</tr>\n\t</thead>\n\t<tbody>\n\t\t{% for item in items %}\n\t\t\t<tr>\n\t\t\t\t<td>{{ item.idx }}</td>\n\t\t\t\t<td>{{ item.item_name }}</td>\n\t\t\t\t<td class=\"text-right\">{{ item.qty }}</td>\n\t\t\t\t<td class=\"text-right\">{{ format_number(item.amount, null,2) }}</td>\n\t\t\t</tr>\n\t\t{% endfor %}\n\t</tbody>\n</table>\n\n<table width=\"100%\" style=\"border-top: 1px solid black\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t<b>{{ __(\"TOTAL\") }} &nbsp  ------------------------------ </b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t<b>{{ format_number(total, null,2) }}</b>\n\t\t\t</td>\n\t\t</tr>\n\t\t{% for tax in taxes %}\n\t\t\t<tr>\n\t\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t\t<b>{{  __(\"VAT\")  }} &nbsp  ------------------------------</b>\n\t\t\t\t</td>\n\t\t\t\t<td class=\"text-right\">\n\t\t\t\t\t<b>{{ format_number(tax.tax_amount, null,2) }}</b>\n\t\t\t\t</td>\n\t\t\t</tr>\n\t\t{% endfor %}\n\t\t<tr>\n\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t<b>{{ __(\"GRAND TOTAL\") }} &nbsp -------------------------</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t<b>{{ format_number(grand_total, null,2) }}</b>\n\t\t\t</td>\n\t\t</tr>\n\t    <tr>\n\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t<b>{{ __(\"PAID\") }} &nbsp ------------------------------</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ format_number(paid_amount, null,2) }}\n\t\t\t</td>\n\t\t</tr><tr>\n\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t<b>{{ __(\"OUTSTANDING\") }} &nbsp ------------------------</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ format_number(outstanding_amount, null,2) }}\n\t\t\t</td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t<b>{{ __(\"CHANGE\") }} &nbsp ------------------------------</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ format_number(change_amount, null,2) }}\n\t\t\t</td>\n\t\t</tr>\n\t\t{% if discount_amount > 0 %}\n\t\t\t<tr>\n\t\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t\t<b>{{ __(\"DISCOUNT\") }} &nbsp ------------------------------</b>\n\t\t\t\t</td>\n\t\t\t\t<td class=\"text-right\">\n\t\t\t\t\t{{ format_number(discount_amount, null,2) }}\n\t\t\t\t</td>\n\t\t\t</tr>\n\t\t{% endif %}\n\t\t\t{% if write_off_amount > 0 %}\n\n\t\t\t\t<tr>\n\t\t\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t\t\t<b>{{ __(\"WRITE OFF\") }} &nbsp ------------------------------</b>\n\t\t\t\t\t</td>\n\t\t\t\t\t<td class=\"text-right\">\n\t\t\t\t\t\t{{ format_number(write_off_amount, null,2) }}\n\t\t\t\t\t</td>\n\t\t\t\t</tr>\n\t\t\t{% endif %}\n\n\t</tbody>\n</table>\n<table width=\"100%\" style=\"border-top: 1px solid black\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t{{ __(\"TOTAL NO.OF ITEMS:/:\") }}\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ items.length }}\n\t\t\t</td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td class=\"text-left\" style=\"width: 90%\">\n\t\t\t\t{{ posting_date }}\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ posting_time }}\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n<p>{{ terms }}</p>\n<p class=\"text-center\">{{ __(\"Thank you, please visit again.\") }}</p>\n",
